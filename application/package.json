{
	"name": "madeyoulook",
	"private": true,
	"version": "0.1.0",
	"homepage": "http://www.madeyoulook.ca",
	"description": "Made You Look Application",
	"main": "server.js",
	"scripts": {
		"start": "node server.js"
	},
	"files": [
		"api",
		"build",
		"node_modules",
		"public",
		"server.js",
		"source"
	],
	"eslintConfig": {
		"root": true,
		"env": {
			"browser": true,
			"node": true,
			"mongo": true
		},
		"globals": {
			"angular": true
		},
		"plugins": [
			"angular",
			"node"
		],
		"extends": [
			"eslint:recommended",
			"plugin:node/recommended",
			"plugin:angular/johnpapa"
		],
		"rules": {
			"angular/file-name": [2,{"typeSeparator":"dot","nameStyle":"dot","ignoreTypeSuffix":true}]
		}
	},
	"author": {
		"name": "John Spiteri (johnjspiteri)",
		"email": "john@johnspiteri.io",
		"url": "http://johnspiteri.io"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/made-you-look/application"
	},
	"bugs": {
		"url": "https://github.com/made-you-look/application/issues"
	},
	"dependencies": {
		"angular": "1.4.9",
		"angular-animate": "1.4.9",
		"angular-aria": "1.4.9",
		"angular-material": "1.0.0",
		"angular-messages": "1.4.9",
		"angular-resource": "1.4.9",
		"angular-ui-router": "0.2.18",
		"angular-rangeslider": "0.0.14",
		"async": "2.3.0",
		"bcryptjs": "2.3.0",
		"body-parser": "1.14.1",
		"compression": "1.6.2",
		"cookie-parser": "1.0.1",
		"errorhandler": "1.0.0",
		"express": "4.9.0",
		"jquery": "~1.11.2",
		"method-override": "1.0.0",
		"mongoose": "4.3.4",
		"morgan": "1.6.1"
	},
	"devDependencies": {
		"autoprefixer": "6.7.7",
		"cssnano": "3.8.0",
		"eslint": "3.19.0",
		"eslint-plugin-angular": "2.4.0",
		"eslint-plugin-node": "4.2.2",
		"grunt": "1.0.1",
		"grunt-angular-templates": "1.1.0",
		"grunt-cli": "1.2.0",
		"grunt-concurrent": "2.3.1",
		"grunt-contrib-pug": "1.0.0",
		"grunt-contrib-stylus": "1.2.0",
		"grunt-contrib-uglify": "2.3.0",
		"grunt-contrib-watch": "1.0.0",
		"grunt-express-server": "0.5.3",
		"grunt-minjson": "0.4.0",
		"grunt-newer": "1.3.0",
		"grunt-open": "0.2.3",
		"grunt-postcss": "0.8.0",
		"grunt-puglint": "1.0.0",
		"load-grunt-config": "0.19.2",
		"pixrem": "3.0.2",
		"time-grunt": "1.4.0"
	},
	"engines": {
		"node": ">=0.11.0",
		"npm": "4.2.2"
	}
}
